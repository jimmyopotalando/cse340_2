<header id="top-header">
  <span class="siteName">
    <a href="/" title="Return to home page">CSE Motors</a>
  </span>
  <div id="tools">
    <% if (loggedin && accountData) { %>
      <a href="/account/" title="Go to account">Welcome <%= accountData.account_firstname %></a>
      <a href="/account/logout" title="Log out">Logout</a>
      <% } else { %>
        <a href="/account/login" title="Click to log in">My Account</a>
        <% } %>
  </div>
  <nav>
    <ul>
      <li><a href="/">Home</a></li>
      <% if (loggedin && accountData) { %>
        <li><a href="/account/">Dashboard</a></li>
        <li><a href="/account/logout">Logout</a></li>
        <% } else { %>
          <li><a href="/account/login">My Account</a></li>
          <% } %>
    </ul>
  </nav>
</header>